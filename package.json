{
  "name": "automart",
  "version": "1.0.0",
  "description": "automart app",
  "main": "server/app.js",
  "scripts": {
    "build": "npm run clean && mkdir dist && babel server -d dist --copy-files",
    "clean": "rm -rf dist",
    "start": "npm run build && node -r esm /dist/app",
    "start:dev": "nodemon",
    "coverage": "nyc --reporter=text --reporter=html mocha -r @babel/register --recursive -r @babel/polyfill -r dotenv/config server/test/*.js --exit && npm run generate-lcov",
    "lint": "eslint ./server --ext .js",
    "test": "nyc mocha --reporter spec -r @babel/register server/test/*.js --exit",
    "generate-lcov": "nyc report --reporter=text-lcov | coveralls",
    "coveralls-coverage": "lcov.info < coveralls ",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info"
  },
  "keywords": [
    "npm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EricoMartin/automart.git"
  },
  "author": "Ibu Omenka Eric",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "bcrypt": "^3.0.6",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.14.0",
    "core-js": "^3.1.4",
    "coveralls": "^3.0.4",
    "dotenv": "^8.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "pg": "^7.11.0",
    "regenerator-runtime": "^0.13.2",
    "swagger-jsdoc": "^3.3.0",
    "swagger-ui-express": "^4.0.7",
    "which": "^1.3.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": "10.15.3"
  },
  "nodemonConfig": {
    "ignore": [
      "server/test/*.js"
    ],
    "watch": [
      "server"
    ],
    "exec": "node bin/dev",
    "ext": "js"
  }
}
