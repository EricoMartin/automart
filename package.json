{
  "name": "server",
  "version": "1.0.0",
  "description": "An online automobile dealership App",
  "main": "app.js",
  "scripts": {
    "build": "npm run clean && mkdir dist && babel server -d dist --copy-files",
    "clean": "rm -rf dist",
    "lint": "eslint ./api --ext .js",
    "start": "node bin/prod",
    "start:dev": "nodemon",
    "test": "nyc --reporter text --reporter html mocha --reporter spec -r @babel/register -r @babel/polyfill  -r dotenv/config server/test/*.js --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "docs": "./node-modules/.bin/jsdoc -c jsdoc.json"
  },
  "keywords": [],
  "author": "Ibu Omenka Eric",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.14.0",
    "connect-multiparty": "^2.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "fancy-log": "^1.3.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "regenerator-runtime": "^0.13.2",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codecov": "^3.5.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": "10.15.3"
  },
  "nodemonConfig": {
    "ignore": [
      "server/test/*.js"
    ],
    "watch": [
      "server"
    ],
    "exec": "node bin/dev",
    "ext": "js"
  }
}
